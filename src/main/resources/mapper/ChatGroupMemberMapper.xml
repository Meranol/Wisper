<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.wisper_one.websocket.chat_group.mapper.ChatGroupMemberMapper">

    <!-- 插入群成员 -->
    <insert id="insert" parameterType="com.example.wisper_one.websocket.chat_group.POJO.ChatGroupMemberEntity" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO chat_group_member(group_code,user_code, member_nick, role, join_time, mute_until)
        VALUES(#{groupCode}, #{userCode}, #{memberNick}, #{role}, #{joinTime}, #{muteUntil})
    </insert>

</mapper>
